    <ImageView
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="270dp"
        android:src="@drawable/alarmpink" />
    </LinearLayout>




</LinearLayout>