<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/fragment_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="0dp">

            <ImageView
                android:id="@+id/logo"
                android:layout_width="77.7dp"
                android:layout_height="18dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/logo" />

            <ImageView
                android:id="@+id/alram"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="20dp"
                android:layout_gravity="right"
                android:src="@drawable/mypagealram" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="89dp"
                android:fontFamily="@font/gmarketsansbold"
                android:text="MY"
                android:textColor="#494949"
                android:textSize="24dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="130dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="46dp"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/round_button_3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="이루다"
                    android:textColor="#494949"
                    android:textSize="16dp" />

                <LinearLayout
                    android:layout_width="75dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="120dp"
                    android:background="@drawable/backboxgray">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="15dp"
                        android:fontFamily="@font/gmarketsansmedium"
                        android:text="수정하기"
                        android:textSize="12dp"
                        android:textColor="#FBFBFB" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="210dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/card" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="결제 수단 등록"
                    android:textSize="16dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="150dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="&gt;"
                    android:textSize="18dp"
                    android:textColor="#434343" />
            </LinearLayout>
            <View
                android:layout_marginTop="270dp"
                android:layout_width="380dp"
                android:layout_gravity="center_horizontal"
                android:layout_height="1dp"
                android:background="#F4F4F4" />

            <TextView
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gmarketsansmedium"
                android:text="내역"
                android:layout_marginTop="290dp"
                android:layout_marginLeft="20dp"
                android:textColor="#A4A4A4"
                android:textSize="15dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="310dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/review" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="내가 쓴 리뷰"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="365dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/specification" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="내 서비스 명세서"
                    android:textSize="16dp" />
            </LinearLayout>

            <TextView
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gmarketsansmedium"
                android:text="설정"
                android:layout_marginTop="440dp"
                android:layout_marginLeft="20dp"
                android:textColor="#A4A4A4"
                android:textSize="15dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="460dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/setting" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="계정 설정"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="515dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/mypagemenualram" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="알림 설정"
                    android:textSize="16dp" />
            </LinearLayout>

            <TextView
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gmarketsansmedium"
                android:text="기타"
                android:layout_marginTop="590dp"
                android:layout_marginLeft="20dp"
                android:textColor="#A4A4A4"
                android:textSize="15dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="610dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/center" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="고객센터"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="665dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/talk" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="공지사항"
                    android:textSize="16dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="720dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/qna" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="FAQ"
                    android:textSize="16dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="775dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/lougout" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="로그아웃"
                    android:textSize="16dp" />
            </LinearLayout>
        </FrameLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>