<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp">

        <FrameLayout
            android:id="@+id/fragment_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="0dp">

            <ImageView
                android:id="@+id/logo"
                android:layout_width="120dp"
                android:layout_height="31dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="30dp"
                android:src="@drawable/logo" />

            <ImageView
                android:id="@+id/alram"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="right"
                android:layout_marginTop="37dp"
                android:layout_marginRight="25dp"
                android:src="@drawable/mypagealram" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="28dp"
                android:layout_marginTop="94dp"
                android:fontFamily="@font/gmarketsansbold"
                android:text="MY"
                android:textColor="#494949"
                android:textSize="24dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="140dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="46dp"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/round_button_3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="이루다"
                    android:textColor="#494949"
                    android:textSize="16dp" />

                <LinearLayout
                    android:layout_width="75dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="158dp"
                    android:background="@drawable/round_button_3">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="15dp"
                        android:fontFamily="@font/gmarketsansmedium"
                        android:text="수정하기"
                        android:textColor="#FBFBFB"
                        android:textSize="12dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="215dp"
                android:padding="10dp">

                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/cardspink" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="결제 수단 등록"
                    android:textSize="16dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="170dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="&gt;"
                    android:textColor="#434343"
                    android:textSize="18dp" />
            </LinearLayout>

            <View
                android:layout_width="380dp"
                android:layout_height="1.3dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="280dp"
                android:background="#F4F4F4" />

            <TextView
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="290dp"
                android:fontFamily="@font/gmarketsansmedium"
                android:text="내역"
                android:textColor="#A4A4A4"
                android:textSize="15dp" />

            <LinearLayout
                android:id="@+id/linear_layout_group1"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="310dp"
                android:padding="10dp">

                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/review" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="내가 쓴 리뷰"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_layout_group2"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="365dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:layout_gravity="center"
                    android:src="@drawable/specification" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="내 서비스 명세서"
                    android:textSize="16dp" />
            </LinearLayout>

            <TextView
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="440dp"
                android:fontFamily="@font/gmarketsansmedium"
                android:text="설정"
                android:textColor="#A4A4A4"
                android:textSize="15dp" />

            <LinearLayout
                android:id="@+id/linear_layout_group3"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="460dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:layout_gravity="center"
                    android:src="@drawable/setting" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="계정 설정"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_layout_group4"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="515dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:layout_gravity="center"
                    android:src="@drawable/mypagemenualram" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="알림 설정"
                    android:textSize="16dp" />
            </LinearLayout>

            <TextView
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="590dp"
                android:fontFamily="@font/gmarketsansmedium"
                android:text="기타"
                android:textColor="#A4A4A4"
                android:textSize="15dp" />

            <LinearLayout
                android:id="@+id/linear_layout_group5"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="610dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/center" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="고객센터"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_layout_group6"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="665dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/talk" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="공지사항"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_layout_group7"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="720dp"
                android:padding="10dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_gravity="center"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/qna" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="FAQ"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_layout_group8"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_marginTop="775dp"
                android:padding="10dp">

                <ImageView

                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="22dp"
                    android:src="@drawable/lougout" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="25dp"
                    android:fontFamily="@font/gmarketsansmedium"
                    android:text="로그아웃"
                    android:textSize="16dp" />
            </LinearLayout>
        </FrameLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>